<div id="map"></div>
{{!-- Initializing the map to be centered at SFSU --}}
<script>
    function initMap() {
            //coordinates for SFSU
            myLatLng = { lat: 37.7241, lng: -122.4799 };
            map = new google.maps.Map(document.getElementById("map"), {
                zoom: 13,
                center: myLatLng,
            });
                       
            
            
        }
</script>
